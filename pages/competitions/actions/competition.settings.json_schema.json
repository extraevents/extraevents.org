{
    "type": "object",
    "required": [
        "id",
        "organizers",
        "contact",
        "registration_close"
    ],
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^[a-zA-Z0-9_]*$"
        },
        "organizers": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "string",
                "pattern": "^[0-9]{4}[a-zA-Z]{4}[0-9]{2}$"
            }
        },
        "contact": {
            "type": "string",
            "format": "email"
        },
        "registration_close": {
            "type": "string",
            "format": "date-time"
        },
        "events": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "id",
                    "round",
                    "format",
                    "time_limit",
                    "competitor_limit"
                ],
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "round": {
                        "type": "integer",
                        "enum": [1, 2, 3, 4]
                    },
                    "format": {
                        "type": "string",
                        "enum": ["a", "m", "1", "2", "3"]
                    },
                    "cutoff": {
                        "type": "number"
                    }
                    ,
                    "time_limit": {
                        "type": "integer"
                    },
                    "time_limit_cumulative": {
                        "type": "boolean"
                    },
                    "competitor_limit": {
                        "type": "integer"
                    },
                    "settings": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": ["autoteam"]
                        } 
                    }
                }
            }
        }
    }
}